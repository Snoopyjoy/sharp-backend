{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [ "./app.js", "./lib" ],
    "exclude": [ "./test", "./node_modules" ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true
  }
}
